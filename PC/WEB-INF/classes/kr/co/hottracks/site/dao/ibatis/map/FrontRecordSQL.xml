<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd">
<sqlMap namespace="FrontRecordSQL">
    <typeAlias alias="qnADomain" type="kr.co.hottracks.site.model.common.faq.QnADomain" />
    
    <!-- 
        작성자: 장진용
        작성일: 2014-09-01
        기능: 해당 시퀀스의 음반 리뷰 반환
    -->
    <select id="getRecordReviewBySeq" parameterClass="Integer" resultClass="qnADomain">
        /* FrontRecordSQL.getRecordReviewBySeq */
        SELECT RCRD_REVIEW_SEQ      seq
             , RCRD_CD              rcrdCd
             , REVIEW               title
             , GRADE                grade
             , REGST_DTM            regstDtm
             , USE_YN               publicYn
        FROM TB_RECORD_REVIEW
       WHERE RCRD_REVIEW_SEQ = $value$
    </select>
</sqlMap>
